programa {
  funcao inicio() {
    inteiro numero, resposta, vezes

     escreva("escreva um numero: ", resposta)
        leia(resposta)
     
     para(numero=1; numero<=20; numero++){
      vezes=resposta*numero
      escreva("\nseu numero: ",resposta, " tabuada: ", numero, " resultado: ", vezes)

     }numero++
    
    }
  }
}
